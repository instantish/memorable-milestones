{
  "name": "memorable-milestones",
  "version": "2.0.0",
  "private": true,
  "description": "Puts milestones on auto-pilot with memorable emoji names",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instantish/memorable-milestones.git"
  },
  "keywords": [
    "actions",
    "node",
    "milestone"
  ],
  "author": "Instantish",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@octokit/rest": "^18.5.3",
    "moment": "^2.25.1",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/parser": "^4.22.0",
    "@vercel/ncc": "^0.28.3",
    "eslint": "^7.24.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.1.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  }
}
